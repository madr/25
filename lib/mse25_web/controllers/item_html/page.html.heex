<article class="article" vocab="https://schema.org/" typeof="Article">
  <h1 property="name"><%= @heading %></h1>

  <div property="articleBody">
    <%= raw(@contents) %>
  </div>

  <footer>
    <p>
      Senast uppdaterad <time property="dateModified"><%= @updated_at %></time>
    </p>
  </footer>
</article>
